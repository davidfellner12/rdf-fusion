          Projection: productType, <uuid>, <uuid> AS reviewCount
            Projection: productType, INT64_AS_TERM(<uuid>) AS <uuid>
              Aggregate: groupBy=[[productType AS productType]], aggr=[[count(review) AS <uuid>]]
